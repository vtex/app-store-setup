{
  "memory": 256,
  "ttl": 10,
  "timeout": 10,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "setup": {
      "path": "/setup",
      "public": false,
      "policies": [
        {
          "effect": "allow",
          "actions": [
            "post"
          ],
          "principals": [
            "vrn:apps:*:{{account}}:{{workspace}}:app/vtex.channels-setup@*"
          ]
        },
        {
          "effect": "allow",
          "actions": [
            "post"
          ],
          "principals": [
            "vrn:vtex.vtex-id:*:{{account}}:*:user/*@vtex.com.br"
          ]
        }
      ]
    },
    "configuration": {
      "path": "/_v/vtex.app-store-setup/configuration",
      "public": true
    }
  }
}
